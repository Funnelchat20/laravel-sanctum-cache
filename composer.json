{
  "name": "funnelchat20/laravel-sanctum-cache",
  "description": "Custom Sanctum token repository using cache and database",
  "type": "library",
  "autoload": {
    "psr-4": {
      "LaravelSanctumCache\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelSanctumCache\\Providers\\CustomSanctumServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "laravel/sanctum": "^4.0",
    "laravel/framework": "^11.41"
  }
}
